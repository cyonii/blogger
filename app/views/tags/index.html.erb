<h1>All Tags</h1>

<ul id="tags">
  <% @tag.each do |tag| %>
    <li>
      <%= link_to tag.name, tag_path(tag)%>

      <% if logged_in? %>
       <%= link_to "Delete", tag_path(tag), method: :delete, data: {confirm: "Really delete the tag?"} %>
      <% end %>
    </li>
  <% end %>
</ul>

<br><br>

<%= link_to "Create a new Article", new_article_path, class: 'new_articles' %>
